<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layoutt"
   
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/contactusxtra"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="216dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/e9"
        android:layout_alignLeft="@+id/btn2"
        android:layout_marginBottom="174dp"
        android:layout_marginLeft="31dp"
        android:orientation="horizontal" >
    </RelativeLayout>

    <EditText
        android:id="@+id/e5"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="680dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/mobnumb"
        android:inputType="phone"
        android:maxLength="13"
        android:padding="5dip"
        android:singleLine="true" />

    <EditText
        android:id="@+id/e8"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="870dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/city"
        android:padding="5dip"
        android:singleLine="true" />

    <EditText
        android:id="@+id/e4"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="610dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/orgname"
        android:padding="5dip"
        android:singleLine="true" />

    <EditText
        android:id="@+id/e3"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="false"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="540dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/email"
        android:inputType="textEmailAddress"
        android:padding="5dip"
        android:singleLine="true" />

    <EditText
        android:id="@+id/e2"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="470dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/lastname"
        android:padding="5dip"
        android:singleLine="true" />

    <EditText
        android:id="@+id/e1"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="false"
        android:layout_alignParentRight="false"
        android:layout_alignParentTop="false"
        android:layout_centerHorizontal="true"
        android:layout_centerInParent="false"
        android:layout_marginTop="400dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/firstname"
        android:padding="5dip"
        android:singleLine="true" />

    <EditText
        android:id="@+id/e9"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="940dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:hint="@string/state"
        android:padding="5dip"
        android:singleLine="true" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/e9"
        android:layout_marginLeft="222dp"
        android:layout_marginTop="46dp"
        android:background="@drawable/sendextrabut" />

    <Button
        android:id="@+id/btn3"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignBaseline="@+id/btn2"
        android:layout_alignBottom="@+id/btn2"
        android:layout_alignRight="@+id/e9"
        android:background="@drawable/clearmessageextrabut" />

    <EditText
        android:id="@+id/e6"
        style="@style/customcontact"
        android:layout_width="355dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="750dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:gravity="top|left"
        android:hint="@string/addrs"
        android:maxLines="5"
        android:padding="5dip"
        android:textSize="16sp" />

</RelativeLayout>